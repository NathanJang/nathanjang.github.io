{
  "name": "jonathanchan-me",
  "version": "2025.06.28",
  "description": "My personal website.",
  "license": "MIT",
  "author": "Jonathan Chan <hi@jonathanchan.me>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/nathanjang/nathanjang.github.io",
  "scripts": {
    "build": "ember build",
    "build-prod": "ember build -prod",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --cache",
    "start": "ember serve",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"yarn:lint:css -- --fix\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\" --prefixColors auto && yarn lint:format:fix",
    "lint:format": "prettier . --cache --check",
    "lint:format:fix": "prettier . --cache --check --write",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js:fix": "eslint . --fix",
    "commit-prod": "yarn build-prod && ember github-pages:commit --branch master --destination . --message \"Deploy $(git rev-parse --short HEAD)\""
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@embroider/macros": "^1.18.0",
    "@eslint/js": "^9.30.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "bootstrap": "^5.3.7",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "^6.5.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^5.5.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "^6.5.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.9.1",
    "ember-welcome-page": "^7.0.2",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-qunit": "^8.2.2",
    "globals": "^16.2.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.0.6",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "tracked-built-ins": "^4.0.0",
    "webpack": "^5.99.9",
    "yarn-audit-fix": "^10.1.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "private": true,
  "ember": {
    "edition": "octane"
  }
}
