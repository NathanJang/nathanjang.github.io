<div class="experience-container">
  <h1 class="experience-heading">
    What do I do?
    <br>
    <small class="experience-caption">Here are some links!</small>
  </h1>
  <div class="project-cards-container container-fluid">
    <div class="row">
      {{#each projects as |project index|}}
        {{project-card project=project projectCardShouldBeFullWidth=(project-card-should-be-full-width index projects) classNames="project-card-cell col-12"}}
      {{/each}}
    </div>
  </div>
</div>
